				jQuery.post(ajaxurl, data_2, function(response) {
					gsa_description.html(response);
				});
				
				var data_3 = {
					'action': 'winner_report_title',
					'award_id': award_id,
				};

				jQuery.post(ajaxurl, data_3, function(response) {
					gsa_report_link_title.html(response);
